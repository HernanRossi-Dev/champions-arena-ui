{
  "extends": "airbnb",
  "rules": {
    "no-unused-vars": 0,
    "quotes": ["off"],
    "comma-dangle": ["off"],
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-console": ["off"],
    "import/extensions": ["off"],
    "no-param-reassign": ["error", { "props": false }],
    "no-alert": ["off"]
  }
}