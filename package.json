{
  "name": "ChampionsArena",
  "version": "1.0.0",
  "description": "A Pathfinder character creation and campaign companion",
  "private": true,
  "scripts": {
    "start": "node ./static/server/server.js",
    "start-hook": "nodemon -w ./static/server/server.js ./static/server/start-hook.js",
    "watch": "webpack-dev-server --hot --inline",
    "watch-dev": "webpack-dev-server --config webpack.dev.js --mode development --hot --inline",
    "compile-server": "babel ./static/server/server.js --out-dir dist --source-maps",
    "watch-server": "babel ./static/server/server.js --out-dir dist --source-maps --watch",
    "dev-all": "npm run watch & npm run watch-server & npm start-hook",
    "lint": "eslint --ext js,jsx src static webpack.config.js",
    "build": "webpack --config webpack.prod.js",
    "build-dev": "webpack --config webpack.dev.js --mode development",
    "component": "mocha --timeout 10000 test/component/*.js --exit"
  },
  "author": "Hernan Rossi",
  "license": "ISC",
